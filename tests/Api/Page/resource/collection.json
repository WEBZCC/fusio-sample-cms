{
  "status": 4,
  "path": "\/page",
  "methods": {
    "GET": {
      "operationId": "get.page",
      "description": "Returns all available pages",
      "tags": [
        "page",
        "testing"
      ],
      "responses": {
        "200": "App_Model_Page_Collection",
        "500": "App_Model_Message"
      }
    },
    "POST": {
      "operationId": "post.page",
      "description": "Creates a new page",
      "security": {
        "app": [
          "page",
          "testing"
        ]
      },
      "tags": [
        "page",
        "testing"
      ],
      "request": "App_Model_Page",
      "responses": {
        "201": "App_Model_Message",
        "500": "App_Model_Message"
      }
    }
  },
  "definitions": {
    "App_Model_Message": {
      "$ref": "Message"
    },
    "App_Model_Page": {
      "$ref": "Page"
    },
    "App_Model_Page_Collection": {
      "$ref": "Page_Collection"
    },
    "Block_Code": {
      "type": "object",
      "properties": {
        "type": {
          "const": "code",
          "type": "string"
        },
        "content": {
          "type": "string"
        }
      }
    },
    "Block_Headline": {
      "type": "object",
      "properties": {
        "type": {
          "const": "headline",
          "type": "string"
        },
        "content": {
          "type": "string"
        }
      }
    },
    "Block_Image": {
      "type": "object",
      "properties": {
        "type": {
          "const": "image",
          "type": "string"
        },
        "src": {
          "type": "string"
        }
      }
    },
    "Block_Paragraph": {
      "type": "object",
      "properties": {
        "type": {
          "const": "paragraph",
          "type": "string"
        },
        "content": {
          "type": "string"
        }
      }
    },
    "Block_Post": {
      "type": "object",
      "properties": {
        "type": {
          "const": "post",
          "type": "string"
        }
      }
    },
    "Collection": {
      "type": "object",
      "properties": {
        "totalResults": {
          "type": "integer"
        },
        "entry": {
          "type": "array",
          "items": {
            "$generic": "T"
          }
        }
      }
    },
    "Message": {
      "type": "object",
      "properties": {
        "success": {
          "type": "boolean"
        },
        "message": {
          "type": "string"
        }
      }
    },
    "Page": {
      "type": "object",
      "properties": {
        "parentId": {
          "type": "integer"
        },
        "status": {
          "type": "integer"
        },
        "sort": {
          "type": "integer"
        },
        "title": {
          "type": "string"
        },
        "blocks": {
          "type": "array",
          "items": {
            "oneOf": [
              {
                "$ref": "Block_Code"
              },
              {
                "$ref": "Block_Headline"
              },
              {
                "$ref": "Block_Image"
              },
              {
                "$ref": "Block_Paragraph"
              },
              {
                "$ref": "Block_Post"
              }
            ]
          }
        },
        "insertDate": {
          "format": "date-time",
          "type": "string"
        }
      }
    },
    "Page_Collection": {
      "$ref": "Collection",
      "$template": {
        "T": "Page"
      }
    }
  }
}